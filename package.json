{
  "name": "wrtshirts",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "lint": "ng lint",
    "docs": "npm run docs:build",
    "format:fix": "pretty-quick --staged",
    "precommit": "run-s format:fix",
    "prepush": "run-s format:fix",
    "format:check": "prettier --config ./.prettierrc --list-different \"src/**/*{.js,.json,.css,.scss}\"",
    "format:all": "prettier --config ./.prettierrc --write \"src/**/*{.js,.json,.css,.scss}\""
  },
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "husky": "^0.14.3",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.12.1",
    "pretty-quick": "^1.5.0"
  }
}
